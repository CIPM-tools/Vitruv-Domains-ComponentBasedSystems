import "http://www.eclipse.org/uml2/5.0.0/UML" as uml 
import "http://palladiosimulator.org/PalladioComponentModel/5.2" as pcm

mappings: umlXpcmTest for UML and PCM

mapping RepositoryXPackage {

   map (pcm::Repository repository)
   
   and (uml::Package rootPkg)
      
      
   such that {
   		routine testRoutine(pcm::Repository pcmRepo) {
			match {
				val umlRepositoryPkg = retrieve optional uml::Package corresponding to pcmRepo 
			}
			action{
				call {
				if(!umlRepositoryPkg.present) return
				}
			}
		}
   }

 }