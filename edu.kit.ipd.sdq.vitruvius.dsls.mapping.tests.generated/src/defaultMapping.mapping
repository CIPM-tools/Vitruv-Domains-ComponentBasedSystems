import "http://edu.kit.ipd.sdq.vitruvius.tests.metamodels.allElementTypes2" as ae2
import "http://edu.kit.ipd.sdq.vitruvius.tests.metamodels.allElementTypes" as ae1

default defaultMapping:
create [ ae1.Root ] with [
	equal(root.singleValuedEAttribute, 10) 
]

mapping containment:
map [ ae1.NonRoot as nr1 ] with [ in(nr1, defaultMapping::root.singleValuedContainmentEReference) ]
and [ ae2.NonRoot2 as nr2 ] with [ default-resource(nr2, "nonRoot2.aet2" relative-to defaultMapping::root) ]