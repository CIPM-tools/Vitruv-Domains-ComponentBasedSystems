<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection1"
            name="Rename Model Element">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection2"
            name="Rename Non-Model Element">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection3"
            name="Change Invariant">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjectionsubmenu"
            name="Change Injection">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection4"
            name="Change Pure Modifier">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection5"
            name="Change Helper Modifier">
      </command>
      <command
            id="tools.vitruv.domains.jml.monitorededitor.jml.modelstats1"
            name="Show Model Stats">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="jmlMenu"
               label="JML">
            <menu
                  commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjectionsubmenu"
                  label="Change Injection">
               <command
                     commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection2"
                     label="Rename Non-Model Element"
                     style="push">
               </command>
               <command
                     commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection1"
                     label="Rename Model Element"
                     style="push">
               </command>
               <command
                     commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection3"
                     label="Change Invariant"
                     style="push">
               </command>
               <visibleWhen
                     checkEnabled="true">
                  <with
                        variable="tools.vitruv.domains.jml.monitorededitor.jml.state.jmlrefactorings">
                     <equals
                           value="TRUE">
                     </equals>
                  </with>
               </visibleWhen>
               <menu
                     label="Modifier">
                  <command
                        commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection4"
                        label="Pure"
                        style="push">
                  </command>
                  <command
                        commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection5"
                        label="Helper"
                        style="push">
                  </command>
               </menu>
            </menu>
            <command
                  commandId="tools.vitruv.domains.jml.monitorededitor.jml.modelstats1"
                  label="Show Model Stats"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.changeinjection.JMLInjectRenameModelElement"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection1">
      </handler>
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.changeinjection.JMLInjectRenameNonModelElement"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection2">
      </handler>
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.changeinjection.JMLInjectChangeInvariantExpression"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection3">
      </handler>
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.changeinjection.JMLInjectChangePureModifier"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection4">
      </handler>
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.changeinjection.JMLInjectChangeHelperModifier"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.changeinjection5">
      </handler>
      <handler
            class="tools.vitruv.domains.jml.monitorededitor.JMLNumberOfSpecificationStatements"
            commandId="tools.vitruv.domains.jml.monitorededitor.jml.modelstats1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <sourceProvider
            provider="tools.vitruv.domains.jml.monitorededitor.changeinjection.CommandState">
         <variable
               name="tools.vitruv.domains.jml.monitorededitor.jml.state.jmlrefactorings"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>
   <extension
         point="tools.vitruv.applications.jmljava.extensions.sourcedirprovider">
      <provides
            provider="tools.vitruv.domains.jml.monitorededitor.JMLMonitoredEditor$JmlSourceDirProvider">
      </provides>
   </extension>

</plugin>
