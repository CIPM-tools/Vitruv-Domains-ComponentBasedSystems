<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="MIRintermediate" nsURI="http://www.kit.edu/ipd/sdq/vitruvius/framework/mir/MIRintermediate"
    nsPrefix="MIRintermediate">
  <eClassifiers xsi:type="ecore:EClass" name="MIR">
    <eStructuralFeatures xsi:type="ecore:EReference" name="classMappings" upperBound="-1"
        eType="#//ClassifierMapping" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="featureMappings" upperBound="-1"
        eType="#//FeatureMapping" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="predicates" upperBound="-1"
        eType="#//Predicate" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="configuration" lowerBound="1"
        eType="#//Configuration" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="packages" lowerBound="2"
        upperBound="2" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EPackage"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mapping" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="predicates" upperBound="-1"
        eType="#//Predicate"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FeatureMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" upperBound="-1" eType="#//EClassifierFeature"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" upperBound="-1"
        eType="#//EClassifierFeature" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="classifierMapping" eType="#//ClassifierMapping"
        eOpposite="#//ClassifierMapping/featureMapping"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="opposite" lowerBound="1"
        eType="#//FeatureMapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ClassifierMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClassifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClassifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="initializer" upperBound="-1"
        eType="#//Initializer" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="featureMapping" eType="#//FeatureMapping"
        eOpposite="#//FeatureMapping/classifierMapping"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="opposite" lowerBound="1"
        eType="#//ClassifierMapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Predicate" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="JavaPredicate" eSuperTypes="#//Predicate">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="checkStatement" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ReverseFeaturesMappedBy" eSuperTypes="#//Predicate">
    <eStructuralFeatures xsi:type="ecore:EReference" name="reverseFeatures" lowerBound="1"
        upperBound="-1" eType="#//EClassifierFeature" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mapping" lowerBound="1"
        eType="#//Mapping"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Initializer" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="JavaInitializer" eSuperTypes="#//Initializer">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="callStatement" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Configuration">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="package" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EClassifierFeature">
    <eStructuralFeatures xsi:type="ecore:EReference" name="eClassifier" lowerBound="1"
        eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClassifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feature" lowerBound="1"
        eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EStructuralFeature"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="definedType" eType="ecore:EClass platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#//EClassifier"/>
  </eClassifiers>
</ecore:EPackage>
